{
  "name": "react-redux-typescript-template",
  "version": "1.0.0",
  "description": "Template Repo for Typescript + React + Redux",
  "main": "index.tsx",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.ts",
    "build": "webpack --mode production --config webpack.prod.ts",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zunaib/react-redux-typescript-template.git"
  },
  "keywords": [
    "typescript",
    "react",
    "redux",
    "react-router",
    "react-dom",
    "es5",
    "es6",
    "es7",
    "connect",
    "sass"
  ],
  "author": "Zunaib Imtiaz",
  "license": "ISC",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/webpack": "^4.41.0",
    "awesome-typescript-loader": "^5.2.1",
    "html-webpack-plugin": "^3.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "bugs": {
    "url": "https://github.com/Zunaib/react-redux-typescript-template/issues"
  },
  "homepage": "https://github.com/Zunaib/react-redux-typescript-template#readme"
